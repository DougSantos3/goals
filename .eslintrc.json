{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "warn",
        "eqeqeq": ["error", "always"],
        "prettier/prettier": "error"
    },
    "ignorePatterns": ["node_modules/", "dist/"]
}
